<?xml version="1.0" ?>
<plugin-upgrade xmlns="http://www.urbancode.com/UpgradeXMLSchema_v1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <migrate to-version="3">
    <migrate-command name="IDTES Authentication">
      <migrate-properties>
        <migrate-property default="${p:step/name}" name="stepID"/>
      </migrate-properties>
    </migrate-command>
  </migrate>
